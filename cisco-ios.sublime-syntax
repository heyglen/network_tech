  partials:
    - match: |
        (?xi)
        (?:
          ^\s*
          (?:
            (?:access-class)|
            (?:address-family)|
            (?:channel-group)|
            (?:default)|
            (?:deny)|
            (?:description)|
            (?:encapsulation)|
            (?:end)|
            (?:exit)|
            (?:exit-address-family)|
