            (?:ipv6)|
            (?:line)|
            (?:logout)|
            (?:name)|
            (?:neighbor)|
            (?:network)|
            (?:no)|
            (?:permit)|
            (?:port-channel)|
            (?:rd)|
            (?:route-target)|
            (?:router)|
            (?:shutdown)|
            (?:snmp-server)|
            (?:standby)|
            (?:switchport)|
            (?:transport)|
            (?:vlan)|
            (?:vrf)|
            (?:write)
          )
          \s
          [^$]*
        )
      scope: text.network.cisco.partial

