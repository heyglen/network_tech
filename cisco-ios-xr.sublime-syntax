        (?:
          (\[)
          (?:\s*(\S+)\s*(,)\s*){1,16}
          (])
          \s+
        )?
        (
          (?:in)|
          (?:out)
        )\s*$
      captures:
        1: support.constant
        2: string.unquoted
        3: comment
        4: string.unquoted
        5: comment
        6: comment
        7: support.constant

