  rpl_edit_prefix_set:
    - match: |
        (?xi)
        ^\s*(edit)\s+(prefix-set)\s+(?=\S+\s+inline)
      captures:
        1: keyword.control
        2: constant.character
      push: [rpl_edit_prefix_inline_entries, rpl_edit_editor]
