        ({{number_32_bit}})\s+
        ({{number_32_bit}})\s+
        ({{number_8_bit}})\s+
        ({{number_8_bit}})\s+
        ({{number_16_bit_wo_zero}})
      captures:
        1: entity.other.attribute-name
        2: support.constant
        3: constant.numeric
        4: constant.numeric
        5: constant.numeric
        6: constant.numeric
        7: constant.numeric

